<header id="full">
  <h3>API Key</h3>
    <p class="view"><a href="{{unbound webservices_baseurl_users_signup}}">Get an API Key</a></p>
    <p class="view"><a href="{{unbound webservices_baseurl}}">Manage Your API Key</a></p>

  <h3>Top APIs</h3>
  {{#each item in topApisNavItems}}
    <p class="view"><a href="{{unbound developerportalUrl}}/{{unbound item.path}}">{{item.text}}</a></p>
  {{/each}}
  <h3>API Resources</h3>
  {{#each item in apiResourcesNavItems}}
    <p class="view"><a href="{{unbound developerportalUrl}}/{{unbound item.path}}">{{item.text}}</a></p>
  {{/each}}
  </p>
</header>
