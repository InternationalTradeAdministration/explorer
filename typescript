#!/bin/bash

# This script is used to sync the
# govwizely and ita explorer 
# github repositories

git clone git@github.com:GovWizely/explorer.git new-ita-explorer
git clone git@github.com:InternationalTradeAdministration/explorer.git old-ita-explorer

cd new-ita-explorer
rm -rf .git README.md .travis.yml

cd ../old-ita-explorer
cp -Rp .git README.md .travis.yml ../new-ita-explorer
cp -p config/environment.js ../new-ita-explorer/config/

cd ../new-ita-explorer
git config user.name "ITA Data Services"
git config user.email "DataServices@trade.gov"
git add -A
git commit -m "Update"
